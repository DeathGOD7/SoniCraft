{
  "multipart": [
    {
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "sonicraft:block/studiopolis_iron_bars_noside",
        "y": 270
      }
    }
  ]
}